<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="au.id.swords.third2"
          android:versionCode="4"
          android:versionName="2.0">
    <uses-sdk android:minSdkVersion="9" android:targetSdkVersion="25"/>
    <application android:label="@string/app_name"
                 android:icon="@drawable/app_48"
                 android:theme="@style/Theme.AppCompat.Light.NoActionBar"
                 android:allowBackup="true">
        <activity android:name="au.id.swords.third2.ThirdActivity"
                  android:label="@string/app_name"
                  android:icon="@drawable/app_48"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="au.id.swords.third2.ThirdNamePreset"/>
        <activity android:name="au.id.swords.third2.ThirdNameProfile"/>
        <activity android:name="au.id.swords.third2.ThirdDelProfile"/>
        <activity android:name="au.id.swords.third2.ThirdAddInclude"/>
    </application>
</manifest> 
